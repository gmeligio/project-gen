  },
  "devDependencies": {
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "cdktf": "^0.20.7",
    "commit-and-tag-version": "^12",
    "constructs": "10.3.0",
    "eslint": "^9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsii": "~5.7.0",
    "jsii-diff": "^1.98.0",
    "jsii-docgen": "^10.5.0",
    "jsii-pacmak": "^1.98.0",
    "jsii-rosetta": "~5.7.0",
    "prettier": "^3.2.5",
    "projen": "0.91.28",
    "ts-jest": "^29.1.2",
    "ts-node": "beta",
    "typescript": "~5.7.0"
  },
  "peerDependencies": {
    "constructs": "^10.3.0",
    "projen": "^0.91.28"
  },
  "dependencies": {
    "uuid": "^11.0.0",
    "yaml": "^2.4.2"
  },
  "bundledDependencies": [
    "uuid",
    "yaml"
  ],
  "pnpm": {},
  "keywords": [
    "projen"
  ],
  "engines": {
    "node": ">= 20.17.0"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "jest": {
    "coverageProvider": "v8",
    "clearMocks": true,
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupJest.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "test/**/*.test.ts",
      "<rootDir>/@(src|test)/**/*(*.)@(spec|test).ts?(x)",
      "<rootDir>/@(src|test)/**/__tests__/**/*.ts?(x)",
      "<rootDir>/@(projenrc)/**/*(*.)@(spec|test).ts?(x)",
      "<rootDir>/@(projenrc)/**/__tests__/**/*.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".d.ts",
      ".js"
    ],
    "transform": {
      "^.+\\.[t]sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ],
      "\\.[jt]sx?$": "@swc/jest"
    },
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test_report",
          "addFileAttribute": "true"
        }
      ]
    ],
    "coverageDirectory": "coverage_report",
    "coverageReporters": [
      "cobertura",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "errorOnDeprecated": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".d.ts",
      ".js"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    },
    "excludeTypescript": [
      "src/**/*.test.ts"
    ]
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1",
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
