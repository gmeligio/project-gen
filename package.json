    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^18",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "cdktf": "^0.20.7",
    "constructs": "10.3.0",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsii": "~5.4.0",
    "jsii-diff": "^1.98.0",
    "jsii-docgen": "^10.4.9",
    "jsii-pacmak": "^1.98.0",
    "jsii-rosetta": "~5.4.0",
    "prettier": "^3.2.5",
    "projen": "0.81.13",
    "standard-version": "^9",
    "ts-jest": "^29.1.2",
    "ts-node": "beta",
    "typescript": "~5.4.0"
  },
  "peerDependencies": {
    "constructs": "^10.3.0",
    "projen": "^0.81.13"
  },
  "dependencies": {
    "uuid": "^9.0.1",
    "yaml": "^2.4.2"
  },
  "bundledDependencies": [
    "uuid",
    "yaml"
  ],
  "pnpm": {},
  "keywords": [
    "projen"
  ],
  "engines": {
    "node": ">= 18.17.0"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "jest": {
    "clearMocks": true,
    "coverageProvider": "v8",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupJest.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "test/**/*.test.ts",
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/@(test|src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".d.ts",
      ".js"
    ],
    "transform": {
      "^.+\\.[t]sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ],
      "\\.[jt]sx?$": "@swc/jest"
    },
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test_report",
          "addFileAttribute": "true"
        }
      ]
    ],
    "coverageDirectory": "coverage_report",
    "coverageReporters": [
      "cobertura",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "errorOnDeprecated": true,
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".d.ts",
      ".js"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    },
    "excludeTypescript": [
      "src/**/*.test.ts"
    ]
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903",
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
