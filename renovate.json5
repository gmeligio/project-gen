// ~~ Generated by projen. To modify, edit .projenrc.ts and run "npx projen".
{
  "schedule": [
    "at any time"
  ],
  "extends": [
    ":preserveSemverRanges",
    "config:base",
    "group:allNonMajor",
    "group:recommended",
    "group:monorepos"
  ],
  "packageRules": [
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "groupName": "devDependencies (non-major)"
    }
  ],
  "ignoreDeps": [
    "@types/node",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "eslint",
    "jsii-rosetta",
    "jsii",
    "standard-version",
    "projen"
  ],
  "customManagers": {
    "customType": "regex",
    "fileMatch": [
      "^version\\.json$"
    ],
    "matchStrings": [
      "\"(?<datasource>.*?)\":\\s*{[^}]*}",
      "\"(?<depName>.*?)\":\\s*\"(?<currentValue>.*)\""
    ],
    "versioningTemplate": "docker"
  }
}
